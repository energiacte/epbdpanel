{
  "name": "visorepbd",
  "version": "2.2.0",
  "description": "Web panel for the epbdcalc library",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "stats": "webpack --profile --json > stats.json",
    "statsprod": "NODE_ENV=production webpack -p --profile --json > stats.json",
    "analyze": "webpack-bundle-analyzer stats.json build",
    "clean": "rm -rf build/",
    "buildprod": "NODE_ENV=production webpack --progress --colors -p",
    "builddev": "NODE_ENV=development webpack --progress --colors -d --watch",
    "webpack-server": "webpack-dev-server --hot --inline --colors",
    "web-server": "http-server -p 3000 ./build",
    "prodserver": "npm run web-server | npm run buildprod",
    "bs3": "rm .bootstraprc && cp node_modules/bootstrap-loader/.bootstraprc-3-default .bootstraprc",
    "lint": "eslint --ignore-pattern build/*.js --ext .js,.jsx app/",
    "start": "npm run webpack-server --history-api-fallback",
    "deploy": "NODE_ENV=production webpack -p"
  },
  "keywords": [
    "energy",
    "buildings",
    "CTE",
    "energ√≠a",
    "edificios"
  ],
  "author": "Rafael Villar Burke <pachi@ietcc.csic.es>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "eslint": "^4.6.1",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-sorting": "^0.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "img-loader": "^2.0.0",
    "imports-loader": "^0.7.1",
    "node-libs-browser": "^2.1.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "react-dev-utils": "^4.2.1",
    "react-hot-loader": "^3.1.3",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.19.0",
    "svgo": "^1.0.3",
    "svgo-loader": "^2.0.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-manifest-plugin": "^1.3.2"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "epbdjs": "^7.3.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}
